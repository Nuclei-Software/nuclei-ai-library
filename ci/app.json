{
    "run_config": {
        "target" : "qemu",
        "hardware" : {
            "baudrate": 115200,
            "timeout": 60
        },
        "qemu": {
            "qemu32": "qemu-system-riscv32",
            "qemu64": "qemu-system-riscv64",
            "timeout": 60
        }
    },
    "build_target": "clean all",
    "build_config": {
        "SOC": "evalsoc"
    },
    "checks": {
        "PASS": ["All tests passed"],
        "FAIL": ["MEPC", "fail", "Error"]
    },
    "appdirs": [
        "."
    ],
    "appdirs_ignore": [
        "nuclei_sdk",
        "nuclei-sdk"
    ],
    "appconfig": {
    }
}
